{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prisma": "npx prisma generate && npx prisma migrate deploy",
    "vercel": "npm run prisma && npm run pulumi && npm run build",
    "pulumi": "bash pulumi.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && tsc && rm -rf tsconfig.tsbuildinfo",
    "db:migrate": "npx prisma migrate dev && npx prisma generate",
    "db:reset": "npx prisma migrate dev && npx prisma generate && npx prisma migrate reset",
    "prepare": "husky install",
    "pre-commit": "npm run lint",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.9.1",
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@jonbilous/next-js-rpc": "^1.21.0",
    "@prisma/client": "^4.2.1",
    "@types/uuid": "^8.3.4",
    "async": "^3.2.4",
    "aws-sdk": "^2.1121.0",
    "babel-plugin-superjson-next": "^0.4.3",
    "framer-motion": "^6.3.0",
    "isomorphic-fetch": "^3.0.0",
    "next": "^12.2.3",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.37.0",
    "uuid": "^8.3.2",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.3",
    "@types/async": "^3.2.15",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "17.0.25",
    "@types/ramda": "^0.28.11",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sharp": "^0.30.2",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "prisma": "^4.2.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4",
    "webpack": "^5.72.1"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
